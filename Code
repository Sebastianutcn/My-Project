	@Override
	public void actionPerformed(ActionEvent e) {
		
		//changing the panels background color
		if(e.getSource()==backgroundColor) {
			JColorChooser c=new JColorChooser();
			
			Color color=JColorChooser.showDialog(null, "Pick a color", Color.black);
			this.getContentPane().setBackground(color);
			digits.setBackground(color);
			panelEv.setBackground(color);
			panelFM.setBackground(color);
			panelTOC.setBackground(color);
			panelLength.setBackground(color);
			points.setBackground(color);
			for(int i=0; i<digit.length; i++)
				digit[i].setBorder(BorderFactory.createLineBorder(color));
		}
		
		//changing the buttons color
		if(e.getSource()==buttonColor) {
			JColorChooser c=new JColorChooser();
			
			Color color=JColorChooser.showDialog(null, "Pick a color", Color.black);
			male.setBackground(color);
			female.setBackground(color);
			sc.setBackground(color);
			lc.setBackground(color);
			time.setBackground(color);
			points.setBackground(color);
			for(int i=0; i<digit.length; i++)
				digit[i].setBackground(color);
			for(int i=0; i<events.length; i++)
				events[i].setBackground(color);
			for(int i=0; i<lengths.length; i++)
				lengths[i].setBackground(color);
		}
	
		//changing the foreground color
		if(e.getSource()==foregroundColor) {
			JColorChooser c=new JColorChooser();
			
			Color color=JColorChooser.showDialog(null, "Pick a color", Color.black);
			male.setForeground(color);
			female.setForeground(color);
			sc.setForeground(color);
			lc.setForeground(color);
			time.setForeground(color);
			points.setForeground(color);
			for(int i=0; i<digit.length; i++)
				digit[i].setForeground(color);
			for(int i=0; i<events.length; i++)
				events[i].setForeground(color);
			for(int i=0; i<lengths.length; i++)
				lengths[i].setForeground(color);
		}
		
		//opening the FINA site
		if(e.getSource()==siteFina) {
			String urlString="http://fina.org";
			try {
				Desktop.getDesktop().browse(new URL(urlString).toURI());
			} catch (MalformedURLException e1) {
				// TODO Auto-generated catch block
				e1.printStackTrace();
			} catch (IOException e1) {
				// TODO Auto-generated catch block
				e1.printStackTrace();
			} catch (URISyntaxException e1) {
				// TODO Auto-generated catch block
				e1.printStackTrace();
			}
			//this.open("http://fina.org");
		}
		
		//opening the base times table for short course
		if(e.getSource()==tableSc) {	
			new NewImage(shortc, 600, 900, "Base Times SC");
		}
		
		//opening the base times table for long course
		if(e.getSource()==tableLc) {
			new NewImage(longc, 600, 900, "Base Times LC");		
		}
		
		//reset the window and start a new one
		//warning message for validation
		if(e.getSource()==neww) {
			int dialogButton=JOptionPane.showConfirmDialog (null, "Are you sure?", "WARNING", JOptionPane.YES_NO_CANCEL_OPTION);
            if(dialogButton == JOptionPane.YES_OPTION) {
            	new MyFrame();
    			this.dispose();
    			
	            if(dialogButton == JOptionPane.NO_OPTION) {
	            }
              }
		}
		
		//quitting the window
		if(e.getSource()==quit) {
			this.dispose();
		}
		
		//opening the formula image
		if(e.getSource()==formula) {
			new NewImage(formulaa, 790, 850, "Formula");
		}
		
		//another interface
		//calculating the time based on the points
		if(e.getSource()==newPoints) {
			
			option=1;
			
			time.setVisible(false);
			points.setVisible(false);
			
			panelPoints=new JTextField();
			panelTime=new JTextField();
			
			panelPoints.setBounds(100, 220, 600, 100);
			panelPoints.setFont(new Font("SERIF", Font.ITALIC, 50));
			panelPoints.setBackground(new Color(200, 255, 255));
			panelPoints.setForeground(Color.black);
			panelPoints.setText("0");
			panelPoints.setBorder(null);
			panelPoints.setEditable(false);
			panelPoints.setHorizontalAlignment(JTextField.CENTER);
			
			panelTime.setBounds(100, 350, 600, 150);
			panelTime.setFont(new Font("SERIF", Font.ITALIC, 50));
			panelTime.setBackground(new Color(0, 220, 230));
			panelTime.setForeground(Color.black);
			panelTime.setText("00:00:00");
			panelTime.setHorizontalAlignment(JTextField.CENTER);
			panelTime.setBorder(null);
			panelTime.setEditable(false);
			
			this.add(panelPoints);
			this.add(panelTime);
			
			
		}
		
		//option to save as .txt the information
		if(e.getSource()==save) {
			FileSave fileSave=new FileSave();
			String event=new String(), ev=new String(),leng =new String(),course=new String(), gend=new String();
			
			if(option==0) {
				if(!sc.isEnabled())	course=sc.getText();
				if(!lc.isEnabled())	course=lc.getText();
				
				if(!male.isEnabled())	gend=male.getText();
				if(!female.isEnabled())	gend=female.getText();
				
					if(!events[0].isEnabled())	{
						ev="Free";
						for(int i=0; i<lengths.length; i++) {
							if(!lengths[i].isEnabled())
							leng=lengths[i].getText();
						}
					}
					if(!events[1].isEnabled()) {
						ev="Fly";
						for(int i=0; i<lengths.length; i++) {
							if(!lengths[i].isEnabled())
							leng=lengths[i].getText();
						}
					}
					if(!events[2].isEnabled()) {
						ev="Backstroke";
						for(int i=0; i<lengths.length; i++) {
							if(!lengths[i].isEnabled())
							leng=lengths[i].getText();
						}
					}
					if(!events[3].isEnabled()) {
						ev="Breaststroke";
						for(int i=0; i<lengths.length; i++) {
							if(!lengths[i].isEnabled())
								leng=lengths[i].getText();
						}
					}
					if(!events[4].isEnabled()) {
						ev="Medley";
						for(int i=0; i<lengths.length; i++) {
							if(!lengths[i].isEnabled())
							leng=lengths[i].getText();
						}
					}
					
					event=ev+" "+ gend+" "+leng+" m "+course;
				
				fileSave.initUI(time.getText(), points.getText(), event);
				fileSave.saveToFile();			
			}
			
			else {
				if(!sc.isEnabled())	course=sc.getText();
				if(!lc.isEnabled())	course=lc.getText();
				
				if(!male.isEnabled())	gend=male.getText();
				if(!female.isEnabled())	gend=female.getText();
				
					if(!events[0].isEnabled())	{
						ev="Free";
						for(int i=0; i<lengths.length; i++) {
							if(!lengths[i].isEnabled())
							leng=lengths[i].getText();
						}
					}
					if(!events[1].isEnabled()) {
						ev="Fly";
						for(int i=0; i<lengths.length; i++) {
							if(!lengths[i].isEnabled())
							leng=lengths[i].getText();
						}
					}
					if(!events[2].isEnabled()) {
						ev="Backstroke";
						for(int i=0; i<lengths.length; i++) {
							if(!lengths[i].isEnabled())
							leng=lengths[i].getText();
						}
					}
					if(!events[3].isEnabled()) {
						ev="Breaststroke";
						for(int i=0; i<lengths.length; i++) {
							if(!lengths[i].isEnabled())
								leng=lengths[i].getText();
						}
					}
					if(!events[4].isEnabled()) {
						ev="Medley";
						for(int i=0; i<lengths.length; i++) {
							if(!lengths[i].isEnabled())
							leng=lengths[i].getText();
						}
					}
					
					event=ev+" "+ gend+" "+leng+" m "+course;
				
				fileSave.initUI(panelTime.getText(), panelPoints.getText(), event);
				fileSave.saveToFile();			
			}
			
		}
		
		/*
		if(e.getSource()==jpg) {
		
				SaveImg fileSave=new SaveImg();
			
				fileSave.initUI();
				fileSave.saveToFile();						
			
		}*/
		
		//////////////////Adding +20 to the progress bar if one of these is selected/////////////////////////////////////
		
		if(e.getSource()==sc||e.getSource()==lc) {
			if(courses==0) {
				counter+=20;
				bar.setValue(counter);
				courses=1;
				if(bar.getValue()==100) {
					bar.setString("You are ready");
				}
			}
			if(courses==1)
				bar.setVisible(true);

		}
		if(e.getSource()==male||e.getSource()==female) {
			if(gender==0) {
				counter+=20;
				bar.setValue(counter);
				gender=1;
			}
			if(gender==1)
				bar.setVisible(true);

		}
		for(int i=0; i<events.length; i++)
		if(e.getSource()==events[i]) {
			if(event==0) {
				counter+=20;
				bar.setValue(counter);
				event=1;
				if(bar.getValue()==100) {
					bar.setString("You are ready");
				}
				if(event==1)
					bar.setVisible(true);
			}
		}
		for(int i=0; i<lengths.length; i++)
			if(e.getSource()==lengths[i]) {
				if(length==0) {
					counter+=20;
					bar.setValue(counter);
					length=1;
					if(bar.getValue()==100) {
						bar.setString("You are ready");
					}
					if(length==1)
						bar.setVisible(true);
				}
			}
		
		if(e.getSource()==sc) {
			sc.setEnabled(false);
			if(events[4].isEnabled()==false)	lengths[1].setVisible(true);
			sc.setBackground(new Color(200, 255, 255));
			sc.setFont(new Font("SERIF", Font.BOLD, 20));
			lc.setEnabled(true);
			lc.setBackground(new Color(0, 175, 255));
			lc.setFont(new Font("SERIF", Font.ITALIC, 20));			

		}
		if(e.getSource()==lc) {
			lc.setEnabled(false);
			lc.setBackground(new Color(200, 255, 255));
			lc.setFont(new Font("SERIF", Font.BOLD, 20));
			sc.setEnabled(true);
			sc.setBackground(new Color(0, 175, 255));
			sc.setFont(new Font("SERIF", Font.ITALIC, 20));
		}
		
		////////////////////////////////
		if(e.getSource()==male) {
			male.setEnabled(false);
			male.setBackground(new Color(200, 255, 255));
			male.setFont(new Font("SERIF", Font.BOLD, 20));
			female.setEnabled(true);
			female.setBackground(new Color(0, 175, 255));
			female.setFont(new Font("SERIF", Font.ITALIC, 20));
		}
		if(e.getSource()==female) {
			female.setEnabled(false);
			female.setBackground(new Color(200, 255, 255));
			female.setFont(new Font("SERIF", Font.BOLD, 20));
			male.setEnabled(true);
			male.setBackground(new Color(0, 175, 255));
			male.setFont(new Font("SERIF", Font.ITALIC, 20));
		}
		
		////////////////////////////////
		//setting visible only certain lengths depending on the event selected
		if(e.getSource()==events[0]) {
			for(int i=0;i<5;i++) {
				events[i].setEnabled(true);
				events[i].setBackground(new Color(0, 175, 255));
				events[i].setFont(new Font("SERIF", Font.ITALIC, 20));
			}
			events[0].setEnabled(false);
			events[0].setBackground(new Color(200, 255, 255));
			events[0].setFont(new Font("SERIF", Font.BOLD, 20));
			for(int i=0;i<6;i++) {
				lengths[i].setVisible(true);
			}
		}
		if(e.getSource()==events[1]) {
			for(int i=0;i<5;i++) {
				events[i].setEnabled(true);
				events[i].setBackground(new Color(0, 175, 255));
				events[i].setFont(new Font("SERIF", Font.ITALIC, 20));
			}
			events[1].setEnabled(false);
