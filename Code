			}
			for(int i=3;i<6;i++) {
				lengths[i].setVisible(false);
			}
		}
		if(e.getSource()==events[2]) {
			for(int i=0;i<5;i++) {
				events[i].setEnabled(true);
				events[i].setFont(new Font("SERIF", Font.ITALIC, 20));
				events[i].setBackground(new Color(0, 175, 255));
			}
			events[2].setEnabled(false);
			events[2].setBackground(new Color(200, 255, 255));
			events[2].setFont(new Font("SERIF", Font.BOLD, 20));
			for(int i=0;i<3;i++) {
				lengths[i].setVisible(true);
			}
			for(int i=3;i<6;i++) {
				lengths[i].setVisible(false);
			}
		}
		if(e.getSource()==events[3]) {
