			}
			
			lengths[0].setVisible(false);
			lengths[4].setVisible(false);
			lengths[5].setVisible(false);
		}
		
		////////////////////////////////////////
		//setting enable to false if one length is selected
		if(e.getSource()==lengths[0]) {
			for(int i=0;i<6;i++) {
