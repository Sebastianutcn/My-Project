		if(option==0) {
		for(int i=0; i<9;i++)
			if(e.getSource()==digit[i]) {
				time.setText(time.getText().concat(String.valueOf(i+1)));
			}
		if(e.getSource()==digit[10]) {
			time.setText(time.getText().concat("0"));
		}
		if(e.getSource()==digit[13]) {
			time.setText(time.getText().concat(":"));
		}
		if(e.getSource()==digit[11]) {
			time.setText("");
		}
		if(e.getSource()==digit[9]) {
			String string=time.getText();
			StringBuffer sb=new StringBuffer(string);
			if(sb.length()!=0)
				sb.deleteCharAt(string.length()-1);
			time.setText(sb.toString());
		}
		}
		else {
			for(int i=0; i<9;i++)
				if(e.getSource()==digit[i]) {
					panelPoints.setText(panelPoints.getText().concat(String.valueOf(i+1)));
				}
			if(e.getSource()==digit[10]) {
				panelPoints.setText(panelPoints.getText().concat("0"));
			}
			
			digit[13].setEnabled(false);
			
