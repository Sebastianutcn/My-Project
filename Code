	//function to convert the time minutes:seconds:miliseconds to seconds
	//and checks
	public double convertToSeconds(String sir) {
		
		if(sir.isEmpty())	return -1;
		
		String[] splited=sir.split(":");
		if(splited.length>3)	return -1;
		for(int i=0;i<splited.length;i++) {
			if(splited[i].length()>2)	return -1;
		}
		
		int min=0, sec=0, milisec=0;
		if(splited.length==3) {
			 min=Integer.parseInt(splited[0]);
			 if(min>59)	return -1;
			 sec=Integer.parseInt(splited[1]);
			 if(sec>59)	return -1;
			 milisec=Integer.parseInt(splited[2]);
			 if(milisec>99)return -1;
		}
		if(splited.length==2) {
			 min=0;
			 sec=Integer.parseInt(splited[0]);
			 if(sec>59)	return -1;
			 milisec=Integer.parseInt(splited[1]);
			 if(milisec>99)return -1;
		}
		if (splited.length<2)	return -1;
		
		double result=min*60+sec+milisec*0.01;
		
		return result;
	}
	
	//converting a certain point to seconds based on the base time for that event
	public double convertToSeconds(String points, double base) {
		double result;
		double aux, auxi;
		
		double points1=Double.parseDouble(points);
		
		if(points1>1100)
			return -1;
		
		aux=(points1/1000.0);
		auxi=Math.pow(aux, -1/3);
		
		result=base*auxi;				
		
		return result;
	}
	
	//converting a certain amount of seconds to minutes:seconds:milliseconds 
	public String secondsToTime(double seconds) {
		String result=new String();
		
		int min, sec, milisec;
		double min1, sec1, milisec1;
		
		min=(int) (seconds/60);
		
		sec=(int) (seconds-min*60);
		
		milisec=(int) (seconds-sec-min*60);
		
		if(seconds==-1) {
			return "Incorrect";
		}
		
		result=""+min+":"+sec+":"+milisec+"";
		
		//result=seconds+"";
		
		return result;
	}
	
	//calculating the fina points based on the time and base time
	public int finaPoints(double base, double seconds) {
		
		int point;
		if(seconds!=-1)
			point=(int) (1000*Math.pow((base/seconds), 3));
		else point=-1;
		
		if(point>=1100)	return -1;
		
		return point;
	}
	
	
	//displaying the points that were calculated
	public void display(int point) {
		if(point!=-1) {
			points.setText("");
			points.setText(point+" points");
		}
		else	points.setText("Incorrect");
	}
	
	//filling the progressbar
	public void fill() {
		int i=counter;
		while(i<=counter+20) {
			bar.setValue(i);
			try {
				Thread.sleep(100);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
			i++;
		}
		//bar.setString("DONE!");
	}

	//changing the image
	@Override
	public void mouseClicked(MouseEvent e) {
		if(i==0) {
			img.setIcon(fina);
			img.setBounds(300, -50, 400, 250);
			i=1;
		}
		if(i==1) {
			img.setIcon(swim);
			img.setBounds(220, -50, 600, 250);
			i=0;
		}
		
	}

	@Override
	public void mousePressed(MouseEvent e) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void mouseReleased(MouseEvent e) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void mouseEntered(MouseEvent e) {
			//img.setIcon(swim);
			//img.setBounds(220, -50, 400, 250);	
	}

	@Override
	public void mouseExited(MouseEvent e) {
		// TODO Auto-generated method stub
		//img.setIcon(fina);
		//img.setBounds(300, -50, 400, 250);	
	}
}



///////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
//FileSave

import java.awt.BorderLayout;
import java.awt.Desktop;
import java.awt.Font;
import java.io.File;
import java.io.FileOutputStream;
import java.io.OutputStreamWriter;
import javax.swing.JButton;
import javax.swing.JFileChooser;
import javax.swing.JFrame;
import javax.swing.JPanel;
import javax.swing.JScrollPane;
import javax.swing.JTextArea;

public class FileSave {
  JTextArea textArea;
  JButton save;
  void initUI(String time, String points, String event) {
    JFrame frame = new JFrame(FileSave.class.getSimpleName());
    frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
    
    textArea = new JTextArea(24, 80);
    textArea.setText("For a time of: "+time+"\n You have: "+points+" \n In the event: "+event);
    textArea.setFont(new Font("MV Boli", Font.BOLD, 40));
    
    save = new JButton("Save to file");
    save.addActionListener(e -> saveToFile());
    
    frame.add(new JScrollPane(textArea));
    
    JPanel buttonPanel = new JPanel();
    
    buttonPanel.add(save);
    
    frame.add(buttonPanel, BorderLayout.SOUTH);
    frame.setSize(900, 400);
    frame.setVisible(true);
  }

   void saveToFile() {
    JFileChooser fileChooser = new JFileChooser();
    fileChooser.setBounds(300, 300, 200, 200);
    int retval = fileChooser.showSaveDialog(save);
    if (retval == JFileChooser.APPROVE_OPTION) {
      File file = fileChooser.getSelectedFile();
      if (file == null) {
        return;
      }
      if (!file.getName().toLowerCase().endsWith(".txt")) {
        file = new File(file.getParentFile(), file.getName() + ".txt");
      }
      try {
        textArea.write(new OutputStreamWriter(new FileOutputStream(file),
            "utf-8"));
        Desktop.getDesktop().open(file);
      } catch (Exception e) {
        e.printStackTrace();
      }
    }
  }
}

//////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
//NewImage

import javax.swing.ImageIcon;
import javax.swing.JFrame;
import javax.swing.JLabel;
import javax.swing.JMenuItem;

public class NewImage {
	
	JLabel label;
	JFrame frame=new JFrame();
	
	//opening a new window
	//i.e: formula, base times(from help menu)
	NewImage(ImageIcon y, int w, int l, String title){
		//frame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		label=new JLabel();
		label.setBounds(0, 0, w, l);
		
		label.setIcon(y);
		frame.setTitle(title);
		frame.add(label);
		frame.setLayout(null);
		frame.setBounds(200, 50, w, l);
		frame.setVisible(true);
	}
}


