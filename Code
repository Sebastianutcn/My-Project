			if((splited.length<=3&&splited.length>=2)&&(sir!="00:00:00"&&sir!="00:00")&&!sir.isEmpty()&&points.getText()!="Incorrect"
				&& convertToSeconds(time.getText())!=-1) {
				counter+=20;
				bar.setValue(counter);
				isCorrect=1;
				if(bar.getValue()==100) {
					bar.setString("You are ready");
				}
			}
			else
				if(isCorrect==1&&isWrong!=1) {
					counter-=20;
					bar.setValue(counter);
					bar.setString(bar.getValue()+"%");
					isWrong=1;
				}
		//	}
			/*
			if(option==1) {
				
